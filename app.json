{
  "expo": {
    "name": "pokemon",
    "slug": "pokemon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#28262E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gamonpo.pokemon",
      "versionCode": 3,
      "permissions": []
    },
    "web": {},
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
